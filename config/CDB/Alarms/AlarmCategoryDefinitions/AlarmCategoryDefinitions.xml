<alarm-category-definitions xmlns="urn:schemas-cosylab-com:Alarm:1.0"
                            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                            xsi:schemaLocation="urn:schemas-cosylab-com:Alarm:1.0 ../../../../config/CDB/schemas/laser-definition.xsd">
	<category-links-to-create>
		<alarm-category-link>
			<category>
				<category-definition path="ROOT"/>
			</category>
			<alarm>
				<alarm-definition fault-family="Scheduling" fault-member="SchedArchiveConnAlarm" fault-code="1"/>
			</alarm>
		</alarm-category-link>
	</category-links-to-create>
</alarm-category-definitions>
