	ARRAY.AVAILABLE_TIME as availableTime, sum(ISNULL(SB.EXECUTION_TIME, 0)) as executionTime
	from OUTPUT_SCHED_BLOCK_RESULT as SB right join OUTPUT_ARRAY as ARRAY on SB.ARRAY_REF = ARRAY.ID
