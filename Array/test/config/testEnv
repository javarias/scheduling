# CDB location
# Comment this line if the CDB being used shouldn't be decompressed
# from a tar file.
CDB_PACKED_FILE=./config/CDB.tar.gz
TESTDIR=`pwd`
ACS_CDB=`pwd`/tmp
# ACS_CDB=`pwd`/config
ACS_TMP=`pwd`/tmp
ACS_LOCK=${ACS_TMP}/.running
ACS_INSTANCE=0
ACS_CONTAINERS="ACC/javaContainer"
IDL_FILES_TO_LOAD=SchedulingArray.idl

# Scheduling XML files, used to load the HSQLDB database
# for testing.
SCHED_XML_PACKED_FILE=./config/SCHED_XML_FILES.tar.gz
APRC_WORK_DIR=$TESTDIR/tmp/SCHED_XML_FILES

# Hibernate database configuration
DBDIR=$ACS_TMP/hsqldb
DBNAME=data_model
DBPORT=9001
SQLTOOL_RC_FILE=./config/sqltool.rc

# State system
RUNLOCATION=tst

JAVA_OPTIONS="-Darchive.configFile=$TESTDIR/config/archiveConfig.properties $JAVA_OPTIONS"
JAVA_OPTIONS="-DACS.managerhost=localhost $JAVA_OPTIONS"

# unset DISPLAY
