<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="alma.scheduling.algorithm.results.Result" table="RESULT">
  <id column="TIME" name="time" type="timestamp">
   <generator class="assigned"/>
  </id>
  <property column="ARRAY_NAME" generated="never" lazy="false"
   name="arrayName" type="java.lang.String"/>
  <set cascade="save-update, lock, delete, delete-orphan" name="scores"
   sort="unsorted" table="SBRANK">
   <key column="ID"/>
   <one-to-many class="alma.scheduling.algorithm.sbranking.SBRank"/>
  </set>
 </class>
</hibernate-mapping>
