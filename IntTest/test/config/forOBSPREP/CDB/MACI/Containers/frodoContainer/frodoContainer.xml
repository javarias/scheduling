<?xml version="1.0" encoding="ISO-8859-1"?>
<Container xmlns="urn:schemas-cosylab-com:Container:1.0"
           xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
           xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:log="urn:schemas-cosylab-com:LoggingConfig:1.0"
           Timeout="20.0"
           UseIFR="1"
           ManagerRetry="10"
           ImplLang="java">
  <Autoload>
  </Autoload>
  <LoggingConfig
             centralizedLogger="Log"
             minLogLevel="4"
             dispatchPacketSize="0"
             immediateDispatchLevel="99">
        <log:_ Name="frodoContainer"                minLogLevel="4"  minLogLevelLocal="4" />
        <log:_ Name="jacorb@frodoContainer"         minLogLevel="4"  minLogLevelLocal="4" />
        <log:_ Name="OBOPS_LIFECYCLE_STATE_SYSTEM"  minLogLevel="4"  minLogLevelLocal="4" />
        <log:_ Name="ARCHIVE_CONNECTION"            minLogLevel="4"  minLogLevelLocal="4" />
        <log:_ Name="ARCHIVE_MASTER_COMP"           minLogLevel="4"  minLogLevelLocal="4" />
  </LoggingConfig>
</Container>
