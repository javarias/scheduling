<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="alma.scheduling.datamodel.obsproject">
    <class name="ObsProject" table="OBSPROJECT">
        <id name="id" column="OBSPROJECTID">            
            <generator class="native"/>
        </id>
        <property name="principalInvestigator" column="PI"/>
        <property name="scienceScore" column="SCIENCE_SCORE" />
        <property name="scienceRank" column="SCIENCE_RANK"/>
        <property name="status" column="STATUS"/>
        <property name="totalExecutionTime" column="TOTAL_EXEC_TIME"/>
        <many-to-one name="obsUnit" class="ObsUnit" column="OBSUNITID"
            cascade="save-update" unique="true" not-null="false" lazy="false"/>
    </class>
</hibernate-mapping>