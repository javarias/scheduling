<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="alma.archive.xml.ObsProjectEntity" table="XML_OBSPROJECT_ENTITIES">
<!--		<cache usage="read-only"/> -->
		<id name="uid" type="string">
			<column name="ARCHIVE_UID" />
			<generator class="assigned"></generator>
		</id>
		<property name="xmlDoc" type="alma.archive.OracleXMLType">
			<column name="XML" />
		</property>
	</class>
	<class name="alma.archive.xml.ObsProposalEntity" table="XML_OBSPROPOSAL_ENTITIES">
