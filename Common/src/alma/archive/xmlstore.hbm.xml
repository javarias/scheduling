		</id>
		<property name="xmlDoc" type="alma.archive.OracleXMLType">
			<column name="XML" />
		</property>
	</class>
	<class name="alma.archive.xml.ObsReviewEntity" table="XML_OBSREVIEW_ENTITIES">
<!--		<cache usage="read-only"/> -->
		<id name="uid" type="string">
			<column name="ARCHIVE_UID" />
			<generator class="assigned"></generator>
		</id>
		<property name="xmlDoc" type="alma.archive.OracleXMLType">
			<column name="XML" />
		</property>
	</class>
	<class name="alma.archive.xml.SchedBlockEntity" table="XML_SCHEDBLOCK_ENTITIES">
<!--		<cache usage="read-only"/> -->	
		<id name="uid" type="string">
			<column name="ARCHIVE_UID" />
