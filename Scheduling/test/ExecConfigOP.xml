    	<!-- =========== runlevel-list =========== -->

   <runlevel-list>
     <!--
     Remarks:
         1) Runlevel names are case insensitive
         2) A set of Runlevels is auto-generated for each subsystem.
         For a subsystem named "BlaBla" they are:
             BlaBla:Shutdown
       BlaBla:PreInitialized
       BlaBla:Online
       BlaBla:Operational
       BlaBla:ReInitializing
       BlaBla:PreInitialized_Or_Higher
       BlaBla:Online_Or_Higher
       BlaBla:PreShutdown_Or_Lower
       BlaBla:PreInitialized_Or_Reinitializing
        -->

       <runlevel name="Alma_Operational">
           <needs-runlevel name="_Alma_Online_Or_Higher" />
           <needs-runlevel name="Archive:Operational" />
           <needs-runlevel name="TelCal:Operational" />
           <needs-runlevel name="Scheduling:Operational" />
           <needs-runlevel name="Control:Operational" />
       </runlevel>
     
       <runlevel name="Alma_Online">
           <needs-runlevel name="Archive:Online_Or_Higher" />
           <needs-runlevel name="TelCal:Online" />
           <needs-runlevel name="Scheduling:Online" />
           <needs-runlevel name="Control:Online" />
       </runlevel>


       <runlevel name="_Alma_Online_Or_Higher">
           <needs-runlevel name="_Alma_PreInitialized_Or_Higher" />
           <needs-runlevel name="Archive:Online_or_higher" />
           <needs-runlevel name="TelCal:Online_or_higher" />
           <needs-runlevel name="Scheduling:Online_or_higher" />
           <needs-runlevel name="Control:Online_or_higher" />
       </runlevel>        
       <runlevel name="_Alma_PreInitialized_Or_Higher">
           <needs-runlevel name="Archive:PreInitialized_Or_Higher" />
           <needs-runlevel name="TelCal:PreInitialized_Or_Higher" />
           <needs-runlevel name="Scheduling:PreInitialized_Or_Higher" />
           <needs-runlevel name="Control:PreInitialized_Or_Higher" />
       </runlevel>  

       <runlevel name="_Alma_PreShutdown_Or_Lower">
           <needs-runlevel name="Scheduling:PreShutdown_Or_Lower" />
           <needs-runlevel name="Control:PreShutdown_Or_Lower" />
           <needs-runlevel name="TelCal:PreShutdown_Or_Lower" />
           <needs-runlevel name="Archive:PreShutdown_Or_Lower" />
       </runlevel>


       <runlevel name="Alma_Shutdown">
           <needs-runlevel name="_Alma_PreShutdown_or_Lower" />
           <needs-runlevel name="Scheduling:Shutdown" />
           <needs-runlevel name="Control:Shutdown" />
           <needs-runlevel name="TelCal:Shutdown" />
           <needs-runlevel name="Archive:Shutdown" />
       </runlevel>


       <runlevel name="Alma_ReInitializing">
           <needs-runlevel name="Archive:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="TelCal:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="Scheduling:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="Control:PreInitialized_Or_Reinitializing" />
       </runlevel>


       <runlevel name="Alma_PreInitialized">
           <needs-runlevel name="Archive:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="TelCal:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="Scheduling:PreInitialized_Or_Reinitializing" />
           <needs-runlevel name="Control:PreInitialized_Or_Reinitializing" />
        </runlevel>  
   </runlevel-list>


